#Cursor Colour
*.cursorBlink: 1
*.cursorUnderline: 1
*.cursorColor: #D8DEE9
*.pointerBlank: 1

#Fake Transparency
*.transparent: true
*.shading: 60

#Font size and colour
*.font: xft: monospace: size=10.5
*.letterSpace: -1

#Removes scrollbar
*.scrollBar: false

#Fading
*.fading: 5
*.fadeColor: #ffffff

#Background Colour and Opacity
*.foreground: #D8DEE9
*.background: #2E3440

